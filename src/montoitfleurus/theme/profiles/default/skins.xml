<?xml version="1.0"?>
<object name="portal_skins"
        allow_any="False"
        cookie_persistence="False"
        default_skin="Skin Mon Toit Fleurusien">

    <object name="montoitfleurus_theme_images"
            meta_type="Filesystem Directory View"
            directory="montoitfleurus.theme:skins/montoitfleurus_theme_images"/>
    <object name="montoitfleurus_theme_templates"
            meta_type="Filesystem Directory View"
            directory="montoitfleurus.theme:skins/montoitfleurus_theme_templates"/>
    <object name="montoitfleurus_theme_styles"
            meta_type="Filesystem Directory View"
            directory="montoitfleurus.theme:skins/montoitfleurus_theme_styles"/>

    <skin-path name="Skin Mon Toit Fleurusien"
               based-on="Sunburst Theme">
        <layer name="montoitfleurus_theme_images"
               insert-after="custom"/>
        <layer name="montoitfleurus_theme_templates"
               insert-after="montoitfleurus_theme_images"/>
        <layer name="montoitfleurus_theme_styles"
               insert-after="montoitfleurus_theme_templates"/>
    </skin-path>

</object>
